opam-version: "2.0"
name: "rocq_ring_like"
version: "dev"

depends: [
  "rocq" {>= "9.0.0"}
]

synopsis: "Ring-like algebraic structures for Rocq"
description: """
Provides definitions and basic properties for structures similar to rings, semi-rings and fields, with additional properties (commutativity, completeness, etc).
"""

build: [
  ["%{make}%"]
]
install: [
  ["%{make}%" "install"]
]
remove: [
  ["%{make}%" "uninstall"]
]
